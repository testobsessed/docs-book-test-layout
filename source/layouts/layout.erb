<!doctype html>
<html>
<head>
  <meta charset="utf-8">

  <!-- Always force latest IE rendering engine or request Chrome Frame -->
  <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">

  <!-- Use title if it's in the page YAML frontmatter -->
  <title><%= data.page.title%> | Pivotal CF Docs</title>

  <%= stylesheet_link_tag 'master', :media => 'screen,print' %>
  <%= stylesheet_link_tag 'breadcrumbs', :media => 'screen,print' %>
  <%= stylesheet_link_tag 'search', :media => 'screen,print' %>
  <%= stylesheet_link_tag 'portal-style', :media => 'screen,print' %>
  <%= stylesheet_link_tag 'printable', :media => 'print' %>

  <%= javascript_include_tag  "all" %>
  <link href='/images/favicon.ico' rel='shortcut icon'>

</head>

<body class="<%= page_classes %>">
  <h1>LAST MOD:
    <%= modified_date "%M %d, %Y" %>
  </h1>
  <%= yield_for_subnav %>

  <%= yield %>
</body>
</html>
